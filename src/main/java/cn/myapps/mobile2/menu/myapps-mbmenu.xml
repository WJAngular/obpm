<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="mbmenu" extends="myapps-default" namespace="/mobile2/menu">
		<action name="launchmenu"
			class="cn.myapps.mobile2.menu.MbMenuAction"
			method="doLaunchMenu">
			<result name="success" type="dispatcher">
			    /mobile2/result.jsp
			</result>
		   	<interceptor-ref name="validationWorkflowStack"/>
			<interceptor-ref name="params" />
		</action>
		<action name="searchmenu"
			class="cn.myapps.mobile2.menu.MbMenuAction"
			method="doSearchMenu">
			<result name="success" type="dispatcher">
			    /mobile2/result.jsp
			</result>
		   	<interceptor-ref name="validationWorkflowStack"/>
			<interceptor-ref name="params" />
		</action>
		
		<action name="applicationmenu"
			class="cn.myapps.mobile2.menu.MbMenuAction"
			method="doApplicationMenu">
			<result name="success" type="dispatcher">
			    /mobile2/result.jsp
			</result>
		   	<interceptor-ref name="validationWorkflowStack"/>
			<interceptor-ref name="params" />
		</action>
		
		<action name="pending"
		    class="cn.myapps.mobile2.menu.MbMenuAction"
		    method="doPending">
		    <result name="success" type="dispatcher">
			    /mobile2/result.jsp
			</result>
		   	<interceptor-ref name="validationWorkflowStack"/>
			<interceptor-ref name="params" />
		</action>
		
		<!-- 流程经办 -->
		<action name="processing"
			class="cn.myapps.mobile2.menu.MbMenuAction"
			method="doProcessing">
			<result name="success" type="dispatcher">
			    /mobile2/result.jsp
			</result>
		   	<interceptor-ref name="validationWorkflowStack"/>
			<interceptor-ref name="params" />
		</action>
		<!-- 流程完成 -->
		<action name="finished"
			class="cn.myapps.mobile2.menu.MbMenuAction"
			method="doFinished">
			<result name="success" type="dispatcher">
			    /mobile2/result.jsp
			</result>
		   	<interceptor-ref name="validationWorkflowStack"/>
			<interceptor-ref name="params" />
		</action>
		
		<action name="finished2"
			class="cn.myapps.mobile2.menu.MbMenuAction"
			method="doFinished2">
			<result name="success" type="dispatcher">
			    /mobile2/result.jsp
			</result>
		   	<interceptor-ref name="validationWorkflowStack"/>
			<interceptor-ref name="params" />
		</action>
		
		<action name="launchmenu2"
		    class="cn.myapps.mobile2.menu.MbMenuAction"
		    method="doLaunchMenu2">
		    <result name="success" type="dispatcher">
		        /mobile2/result.jsp
		    </result>
		    <interceptor-ref name="validationWorkflowStack"/>
			<interceptor-ref name="params" />
		</action>
		
		<action name="applicationmenu"
		    class="cn.myapps.mobile2.menu.MbMenuAction"
		    method="doApplicationMenu">
		    <result name="success" type="dispatcher">
		        /mobile2/result.jsp
		    </result>
		    <interceptor-ref name="validationWorkflowStack"/>
			<interceptor-ref name="params" />
		</action>
		
		<action name="applicationList"
		    class="cn.myapps.mobile2.menu.MbMenuAction"
		    method="doApplicationList">
		    <result name="success" type="dispatcher">
		        /mobile2/result.jsp
		    </result>
		    <interceptor-ref name="validationWorkflowStack"/>
			<interceptor-ref name="params" />
		</action>
		
		<action name="applicationList2"
		    class="cn.myapps.mobile2.menu.MbMenuAction"
		    method="doApplicationList2">
		    <result name="success" type="dispatcher">
		        /mobile2/result.jsp
		    </result>
		    <interceptor-ref name="validationWorkflowStack"/>
			<interceptor-ref name="params" />
		</action>
		
	</package>
</struts>