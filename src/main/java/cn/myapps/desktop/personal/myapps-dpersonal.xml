<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="dpersonal" extends="myapps-default" namespace="/desktop/personal">
		<action name="edit"
			class="cn.myapps.desktop.personal.DPersonalAction"
			method="doEdit">
			<result name="success" type="dispatcher">
				/desktop/xml.jsp
			</result>
			<result name="error" type="dispatcher">
				../error.jsp
			</result>
			<result name="input" type="dispatcher">
				../error.jsp
			</result>
		</action>
		<action name="list"
			class="cn.myapps.desktop.personal.DPersonalAction"
			method="doList">
			<result name="success" type="dispatcher">
				/desktop/xml.jsp
			</result>
			<result name="error" type="dispatcher">
				../error.jsp
			</result>
			<result name="input" type="dispatcher">
				../error.jsp
			</result>
		</action>
		<action name="save"
			class="cn.myapps.desktop.personal.DPersonalAction"
			method="doSave">
			<result name="success" type="dispatcher">
				/desktop/xml.jsp
			</result>
			<result name="error" type="dispatcher">
				../error.jsp
			</result>
			<result name="input" type="dispatcher">
				../error.jsp
			</result>
		</action>
		<action name="delete"
			class="cn.myapps.desktop.personal.DPersonalAction"
			method="doDelete">
			<result name="success" type="dispatcher">
				/desktop/xml.jsp
			</result>
			<result name="error" type="dispatcher">
				../error.jsp
			</result>
			<result name="input" type="dispatcher">
				../error.jsp
			</result>
		</action>
	</package>

	
</struts>