<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<include file="webwork-default.xml" />
	<package name="links" extends="myapps-default"
		namespace="/core/links">
		<action name="edit"
			class="cn.myapps.core.links.action.LinkAction"
			method="doEdit">
			<result name="success" type="dispatcher">
				content.jsp
			</result>		
			<interceptor-ref name="params" />
		</action>
		
		<action name="edit2"
			class="cn.myapps.core.links.action.LinkAction"
			method="doEdit">
			<result name="success" type="dispatcher">
				content2.jsp
			</result>		
			<interceptor-ref name="params" />
		</action>
		
		<action name="new"
			class="cn.myapps.core.links.action.LinkAction"
			method="doNew">
			<result name="success" type="dispatcher">
				content.jsp
			</result>
			<interceptor-ref name="params" />
		</action>
		
		<action name="new2"
			class="cn.myapps.core.links.action.LinkAction"
			method="doNew">
			<result name="success" type="dispatcher">
				content2.jsp
			</result>
			<interceptor-ref name="params" />
		</action>
		<action name="list"
			class="cn.myapps.core.links.action.LinkAction"
			method="doList">
			<result name="success" type="dispatcher">
				list.jsp
			</result>
			<result name="input" type="dispatcher">
				list.jsp
			</result>
			<interceptor-ref name="params" />
			<interceptor-ref name="chain" />
		</action>
		<action name="selectlink"
			class="cn.myapps.core.links.action.LinkAction"
			method="doList">
			<result name="success" type="dispatcher">
				selectlink.jsp
			</result>
			<result name="input" type="dispatcher">
				selectlink.jsp
			</result>
			<interceptor-ref name="params" />
			<interceptor-ref name="chain" />
		</action>
		<action name="save"
			class="cn.myapps.core.links.action.LinkAction"
			method="doSave">
			<result name="success" type="dispatcher">
				content.jsp
			</result>
			<result name="input" type="dispatcher">
				content.jsp
			</result>
			<interceptor-ref name="validationWorkflowStack"/>
			<interceptor-ref name="params" />
		</action>
		<action name="saveAndNew"
			class="cn.myapps.core.links.action.LinkAction"
			method="doSaveAndNew">
			<result name="success" type="dispatcher">
				content.jsp
			</result>
			<result name="input" type="dispatcher">
				content.jsp
			</result>
			<interceptor-ref name="validationWorkflowStack"/>
			<interceptor-ref name="params" />
		</action>
		
			<action name="save2"
			class="cn.myapps.core.links.action.LinkAction"
			method="doSave">
			<result name="success" type="dispatcher">
				content2.jsp
			</result>
			<result name="input" type="dispatcher">
				content2.jsp
			</result>
			<interceptor-ref name="validationWorkflowStack"/>
			<interceptor-ref name="params" />
		</action>
		
		<action name="delete"
			class="cn.myapps.core.links.action.LinkAction"
			method="doDelete">
			<result name="success" type="chain">
				list
			</result>
			<result name="input" type="chain">
				list
			</result>
			<interceptor-ref name="params" />
			<interceptor-ref name="chain" />
		</action>
		
		<action name="deleteByselectList"
			class="cn.myapps.core.links.action.LinkAction"
			method="doDelete">
			<result name="success" type="chain">
				selectlink
			</result>
			<result name="input" type="chain">
				selectlink
			</result>
			<interceptor-ref name="params" />
			<interceptor-ref name="chain" />
		</action>
		<action name="saveAndNew"
			class="cn.myapps.core.links.action.LinkAction"
			method="doSaveAndNew">
			<result name="input" type="dispatcher">
				content.jsp
			</result>
			<result name="success" type="dispatcher">
				content.jsp
			</result>
			<!-- interceptor-ref name="params" / -->
			<interceptor-ref name="validationWorkflowStack"/>
		
		</action>
	</package>
</struts>
