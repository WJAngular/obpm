<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="statelabel" extends="myapps-default" namespace="/core/workflow/statelabel">

		<action name="view"
			class="cn.myapps.core.workflow.statelabel.action.StateLabelAction"
			method="doView">
			<result name="success" type="dispatcher">
				content.jsp
			</result>
			<interceptor-ref name="params" />
			<interceptor-ref name="prepare" />
		</action>

		<action name="new"
			class="cn.myapps.core.workflow.statelabel.action.StateLabelAction"
			method="doNew">
			<result name="success" type="dispatcher">
				content.jsp
			</result>
			<result name="input" type="dispatcher">
				content.jsp
			</result>
			<interceptor-ref name="params" />
			<interceptor-ref name="prepare" />
		</action>

		<action name="save"
			class="cn.myapps.core.workflow.statelabel.action.StateLabelAction"
			method="doSave">
			<result name="input" type="dispatcher">content.jsp</result>
			<result name="success" type="dispatcher">
				content.jsp
			</result>
			<interceptor-ref name="validationWorkflowStack" />
			<interceptor-ref name="prepare" />
		</action>

		<action name="saveAndNew"
			class="cn.myapps.core.workflow.statelabel.action.StateLabelAction"
			method="doSaveAndNew">
			<result name="success" type="dispatcher">
				content.jsp
			</result>
			<result name="input" type="dispatcher">
				content.jsp
			</result>
			<interceptor-ref name="params" />
			<interceptor-ref name="prepare" />
		</action>

		<action name="edit"
			class="cn.myapps.core.workflow.statelabel.action.StateLabelAction"
			method="doEdit">
			<result name="success" type="dispatcher">
				content.jsp
			</result>
			<result name="input" type="dispatcher">
				content.jsp
			</result>
			<interceptor-ref name="params" />
			<interceptor-ref name="prepare" />
		</action>

		<action name="list"
			class="cn.myapps.core.workflow.statelabel.action.StateLabelAction"
			method="doList">
			<interceptor-ref name="chain" />
			<interceptor-ref name="basicStack" />
			<result name="success" type="dispatcher">list.jsp</result>
			<result name="input" type="dispatcher">list.jsp</result>
			<interceptor-ref name="params" />
			<interceptor-ref name="prepare" />
		</action>

		<action name="delete"
			class="cn.myapps.core.workflow.statelabel.action.StateLabelAction"
			method="doDelete">
			<result name="success" type="chain">list</result>
			<result name="input" type="chain">list</result>
		</action>
		
		<action name="statelist"
			class="cn.myapps.core.workflow.statelabel.action.StateLabelAction"
			method="doSelectState">
			<result name="success" type="dispatcher">
				 /core/dynaform/activity/selectlist.jsp
			</result>
		</action>
	
	</package>
</struts>