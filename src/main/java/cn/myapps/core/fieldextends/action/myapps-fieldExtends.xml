<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="ufieldextends" extends="myapps-default"
		namespace="/core/fieldextends">
		<action name="new"
			class="cn.myapps.core.fieldextends.action.FieldExtendsAction" method="doNew">
			<result name="input" type="dispatcher">content.jsp</result>
			<result name="success" type="dispatcher">content.jsp</result>
			
			<interceptor-ref name="params" />
		</action>
		<action name="view"
			class="cn.myapps.core.fieldextends.action.FieldExtendsAction" method="doView">
			<result name="input" type="dispatcher">content.jsp</result>
			<result name="success" type="dispatcher">content.jsp</result>
			
			<interceptor-ref name="params" />
		</action>
		<action name="save"
			class="cn.myapps.core.fieldextends.action.FieldExtendsAction" method="doSave">
			<result name="input" type="dispatcher">content.jsp</result>
			<result name="success" type="dispatcher">content.jsp</result>
			
			<interceptor-ref name="params" />
		</action>
		
		<action name="saveAndNew"
			class="cn.myapps.core.fieldextends.action.FieldExtendsAction" method="doSaveAndNew">
			<result name="input" type="dispatcher">content.jsp</result>
			<result name="success" type="dispatcher">content.jsp</result>
			<interceptor-ref name="params" />
		</action>
		
		<action name="list"
			class="cn.myapps.core.fieldextends.action.FieldExtendsAction" method="doList">
			<result name="input" type="dispatcher">extends.jsp</result>
			<result name="success" type="dispatcher">extends.jsp</result>
			<interceptor-ref name="params" />
		</action>
		
		<action name="query"
			class="cn.myapps.core.fieldextends.action.FieldExtendsAction" method="doQuery">
			<result name="input" type="dispatcher">extends.jsp</result>
			<result name="success" type="dispatcher">extends.jsp</result>
			<interceptor-ref name="params" />
		</action>

		<action name="delete"
			class="cn.myapps.core.fieldextends.action.FieldExtendsAction" method="doDelete">
			<result name="input" type="dispatcher">extends.jsp</result>
			<result name="success" type="dispatcher">extends.jsp</result>
			<interceptor-ref name="params" />
		</action>

	</package>

</struts>