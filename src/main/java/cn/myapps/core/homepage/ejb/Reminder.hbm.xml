<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
	>
	<class
		name="cn.myapps.core.homepage.ejb.Reminder"
		table="T_REMINDER"
		>

		<id
			name="id"
			column="ID"
			type="java.lang.String"
			>
			<generator class="assigned">
				<!--  
						  To add non XDoclet generator parameters, create a file named 
						  hibernate-generator-params-Reminder.xml 
						  containing the additional parameters and place it in your merge dir. 
					  --> 
			</generator>
		</id>

		<property
			name="title"
			type="java.lang.String"
			update="true"
			insert="true"
			column="TITLE"
			/>

		<property
			name="formId"
			type="java.lang.String"
			update="true"
			insert="true"
			column="FORMID"
			/>
        
		<property
			name="filterScript"
			type="text"
			update="true"
			insert="true"
			column="FILTERSCRIPT"
			/>
		<property
			name="type"
			type="java.lang.String"
			update="true"
			insert="true"
			column="TYPE"
			/>
		<many-to-one
			name="homepage"
			class="cn.myapps.core.homepage.ejb.HomePage"
			cascade="none"
			fetch="join"
			lazy="false"
			outer-join="auto"
			update="true"
			insert="true"
			column="HOMEPAGE"
			/>

		<property
			name="orderby"
			type="java.lang.String"
			update="true"
			insert="true"
			column="ORDERBY"
			/>

		<property
			name="summaryFieldNames"
			type="java.lang.String"
			update="true"
			insert="true"
			column="SUMMARYFIELDNAMES"
			/>

		<property
			name="moduleid"
			type="java.lang.String"
			update="true"
			insert="true"
			column="MODULEID"
			/>

		<property
			name="sortId"
			type="java.lang.String"
			update="true"
			insert="true"
			column="SORTID"
			/>

		<property
			name="applicationid"
			type="java.lang.String"
			update="true"
			insert="true"
			column="APPLICATIONID"
			/>
		<property
			name="style"
			type="java.lang.String"
			update="true"
			insert="true"
			column="STYLE"
			/>
		<!--
				To add non XDoclet property mappings, create a file named
					hibernate-properties-Reminder.xml
				containing the additional properties and place it in your merge dir.
			-->

	</class>

</hibernate-mapping>
