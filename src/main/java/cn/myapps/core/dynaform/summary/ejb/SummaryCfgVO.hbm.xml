<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
	>
	<class
		name="cn.myapps.core.dynaform.summary.ejb.SummaryCfgVO"
		table="T_SUMMARY"
		dynamic-update="true"
		>

		<id
			name="id"
			column="ID"
			type="java.lang.String"
			>
			<generator class="assigned">
				<!--  
						  To add non XDoclet generator parameters, create a file named 
						  hibernate-generator-params-Reminder.xml 
						  containing the additional parameters and place it in your merge dir. 
					  --> 
			</generator>
		</id>

		<property
			name="title"
			type="java.lang.String"
			update="true"
			insert="true"
			column="TITLE"
			/>

		<property
			name="formId"
			type="java.lang.String"
			update="true"
			insert="true"
			column="FORMID"
			/>
        
		<property
			name="summaryScript"
			type="text"
			update="true"
			insert="true"
			column="SUMMARYSCRIPT"
			/>
		<property
			name="type"
			type="java.lang.String"
			update="true"
			insert="true"
			column="TYPE"
			/>

		<property
			name="fieldNames"
			type="java.lang.String"
			update="true"
			insert="true"
			column="FIELDNAMES"
			/>


		<property
			name="sortId"
			type="java.lang.String"
			update="true"
			insert="true"
			column="SORTID"
			/>

		<property
			name="applicationid"
			type="java.lang.String"
			update="true"
			insert="true"
			column="APPLICATIONID"
			/>
			
		<property name="scope" 
			type="java.lang.Integer" 
			update="true" insert="true">
			<column name="SCOPE" 
				default="0"
			/>
		</property>
			
