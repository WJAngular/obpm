<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="columnmapping" extends="myapps-default"
		namespace="/core/dynaform/dts/exp/mappingconfig">

		<action name="list"
			class="cn.myapps.core.dynaform.dts.exp.mappingconfig.action.MappingConfigAction"
			method="doList">
      
			<result name="success" type="dispatcher">list.jsp</result>
			<interceptor-ref name="params" />
			<interceptor-ref name="prepare" />
		</action>

		<action name="view"
			class="cn.myapps.core.dynaform.dts.exp.mappingconfig.action.MappingConfigAction"
			method="doView">

			<!-- 			<interceptor-ref name="basicStack" />  -->
			<result name="success" type="dispatcher">
				content.jsp
			</result>
			<interceptor-ref name="params" />
			<interceptor-ref name="prepare" />
		</action>

		<action name="new"
			class="cn.myapps.core.dynaform.dts.exp.mappingconfig.action.MappingConfigAction"
			method="doNew">
			<result name="success" type="dispatcher">
				content.jsp
			</result>
			<interceptor-ref name="params" />
			<interceptor-ref name="prepare" />
		</action>

		<action name="save"
			class="cn.myapps.core.dynaform.dts.exp.mappingconfig.action.MappingConfigAction"
			method="doSave">
			<result name="input" type="dispatcher">content.jsp</result>
			<result name="success" type="dispatcher">
				content.jsp
			</result>
			<interceptor-ref name="validationWorkflowStack" />
		</action>

		<action name="edit"
			class="cn.myapps.core.dynaform.dts.exp.mappingconfig.action.MappingConfigAction"
			method="doView">
			<result name="success" type="dispatcher">
				content.jsp
			</result>
			<interceptor-ref name="params" />
			<interceptor-ref name="prepare" />
		</action>

		<action name="delete"
			class="cn.myapps.core.dynaform.dts.exp.mappingconfig.action.MappingConfigAction"
			method="doDelete">
			<result name="success" type="chain">list</result>
			<result name="input" type="chain">list</result>
			<interceptor-ref name="params" />
			<interceptor-ref name="prepare" />
		</action>
		<action name="exportAllDocument"
			class="cn.myapps.core.dynaform.dts.exp.mappingconfig.action.MappingConfigAction"
			method="exportAllDocument">
			<result name="success" type="dispatcher">
				list.action
			</result>
			<interceptor-ref name="params" />
			<interceptor-ref name="prepare" />
		</action>
				<action name="IncrementExportDocument"
			class="cn.myapps.core.dynaform.dts.exp.mappingconfig.action.MappingConfigAction"
			method="IncrementExportDocument">
			<result name="success" type="dispatcher">
				list.action
			</result>
			<interceptor-ref name="params" />
			<interceptor-ref name="prepare" />
		</action>
	</package>
</struts>
