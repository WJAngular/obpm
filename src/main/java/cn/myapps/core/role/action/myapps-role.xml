<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
<package name="role" extends="myapps-default"
		namespace="/core/role">
		<action name="new" class="cn.myapps.core.role.action.RoleAction"
			method="doNew">
			<result name="success" type="dispatcher">
				content.jsp
			</result>
			<interceptor-ref name="params" />
		</action>
		
		<action name="create" class="cn.myapps.core.role.action.RoleAction"
			method="doNew">
			<result name="success" type="dispatcher">
				content1.jsp
			</result>
			<interceptor-ref name="params" />
		</action>
		<action name="saveRole"
			class="cn.myapps.core.role.action.RoleAction" method="doSave">
			<result name="input" type="dispatcher">
				content1.jsp
			</result>
			<result name="success" type="dispatcher">
				content1.jsp
			</result>
			<interceptor-ref name="params" />
			<interceptor-ref name="validationWorkflowStack"/>
			
		</action>
		<action name="saveAndNew"
			class="cn.myapps.core.role.action.RoleAction" method="doSaveAndNew">
			<result name="input" type="dispatcher">
				content.jsp
			</result>
			<result name="success" type="dispatcher">
				content.jsp
			</result>
			<interceptor-ref name="params" />
			<interceptor-ref name="validationWorkflowStack"/>
			
		</action>
		<action name="saveAndNewRole"
			class="cn.myapps.core.role.action.RoleAction" method="doSaveAndNew">
			<result name="input" type="dispatcher">
				content1.jsp
			</result>
			<result name="success" type="dispatcher">
				content1.jsp
			</result>
			<interceptor-ref name="params" />
			<interceptor-ref name="validationWorkflowStack"/>
			
		</action>
		<action name="listRoles"
			class="cn.myapps.core.role.action.RoleAction" method="doList">
			<interceptor-ref name="chain" />
			<interceptor-ref name="basicStack" />
			<result name="success" type="dispatcher">list.jsp</result>
			<result name="input" type="dispatcher">list.jsp</result>
			<interceptor-ref name="params" />
			<interceptor-ref name="prepare" />
		</action>
		<action name="deleteRole"
			class="cn.myapps.core.role.action.RoleAction" method="doDelete">
			<interceptor-ref name="params" />
			<interceptor-ref name="basicStack" />
			<result name="success" type="chain">listRoles</result>
			<result name="input" type="chain">listRoles</result>
		</action>
		<action name="editRole"
			class="cn.myapps.core.role.action.RoleAction" method="doEdit">
			<result name="success" type="dispatcher">
				content.jsp
			</result>
			<interceptor-ref name="params" />
		</action>
		<action name="edit"
			class="cn.myapps.core.role.action.RoleAction" method="doEdit">
			<result name="success" type="dispatcher">
				content.jsp
			</result>
			<interceptor-ref name="params" />
		</action>
		<action name="save"
			class="cn.myapps.core.role.action.RoleAction" method="doSave">
			<result name="input" type="dispatcher">
				content.jsp
			</result>
			<result name="success" type="dispatcher">
				content.jsp
			</result>
			<interceptor-ref name="params" />
			<interceptor-ref name="validationWorkflowStack"/>
			
		</action>
		<action name="delete"
			class="cn.myapps.core.role.action.RoleAction" method="doDelete">
			<interceptor-ref name="params" />
			<interceptor-ref name="basicStack" />
			<result name="success" type="chain">list</result>
			<result name="input" type="chain">list</result>
		</action>
		<action name="list"
			class="cn.myapps.core.role.action.RoleAction" method="doList">
			<interceptor-ref name="chain" />
			<interceptor-ref name="basicStack" />
			<result name="success" type="dispatcher">list.jsp</result>
			<result name="input" type="dispatcher">list.jsp</result>
			<interceptor-ref name="params" />
			<interceptor-ref name="prepare" />
		</action>
		
		<action name="linkRole"
			class="cn.myapps.core.role.action.RoleAction" method="doList">
			<interceptor-ref name="chain" />
			<interceptor-ref name="basicStack" />
			<result name="success" type="dispatcher">linkroles.jsp</result>
			<result name="input" type="dispatcher">linkroles.jsp</result>
			<interceptor-ref name="params" />
			<interceptor-ref name="prepare" />
		</action>
		
		<action name="rolelist"
			class="cn.myapps.core.role.action.RoleAction" method="doList">
			<result name="success" type="dispatcher">
				selectlist.jsp
			</result>
			<result name="input" type="dispatcher">selectlist.jsp</result>
		</action>
		
		<action name="get-nodes" 
			class="cn.myapps.core.role.action.RoleAction" 
			method="getNodes">
            <result name="success">get-nodes.jsp</result>
        </action>
        
        <action name="rolesListByApplicationJSON" class="cn.myapps.core.role.action.RoleAction" 
			method="doRolesListByApplicationJSON">
        </action>
        
	</package>
	
</struts>