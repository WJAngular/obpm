<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="cn.myapps.core.deploy.module.ejb.ModuleVO"
        table="T_MODULE"
        lazy="false"
    >

        <id
            name="id"
            column="ID"
            type="java.lang.String"
        >
            <generator class="assigned">
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-ModuleVO.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <many-to-one
            name="application"
            class="cn.myapps.core.deploy.application.ejb.ApplicationVO"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="APPLICATION"
        />

        <property
            name="lastmodifytime"
            type="java.util.Date"
            update="true"
            insert="true"
            column="LASTMODIFYTIME"
        />

        <property
            name="description"
            type="java.lang.String"
            update="true"
            insert="true"
            column="DESCRIPTION"
        />

        <property
            name="ispublished"
            type="boolean"
            update="true"
            insert="true"
            column="ISPUBLISHED"
        />

        <property
            name="name"
            type="java.lang.String"
            update="true"
            insert="true"
            column="NAME"
        />

        <property
            name="orderno"
            type="int"
            update="true"
            insert="true"
            column="ORDERNO"
        />

        <many-to-one
            name="superior"
            class="cn.myapps.core.deploy.module.ejb.ModuleVO"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="SUPERIOR"
            not-null="false"
        />

